{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/community-php"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public"
}